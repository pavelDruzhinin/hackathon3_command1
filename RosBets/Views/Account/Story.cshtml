
        <div class="changepass-box container" id="back-layer" color="white">
            Тип пари:
            <select name="bets" id="bets">
                <option value="all" selected>Все</option>
                <option value="ordinary">Ординары</option>
                <option value="express">Экспрессы</option>
            </select>

            Результаты:
            <select name="results" id="results">
                <option value="all" selected>Все</option>
                <option value="positive">Положительные</option>
                <option value="negative">Отрицательные</option>
                <option value="awaiting">Ожидающие ответа</option>
            </select>

            <button name="show" id="show">показать</button>
        </div>

        <div class="panel-group toggle-block">
            <div id="historyTable" class="panel panel-default">
                @Html.Action("CategorySearch", "Account")
            </div>
        </div>

